{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module",
        "allowImportExportEverywhere": false,
        "codeFrame": false
  },
    "rules": {
        "react/jsx-filename-extension": 0,
        "no-labels": 0,
        "no-restricted-syntax": 0
    }
}